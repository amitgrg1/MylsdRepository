#this statement import class Student from student.py file
from student import Student

def main():
#this statement creates an object of student
	std1 = Student()
#print details by calling showDetails function define in Student class
	std1.showDetails()

	print()
#this function set/change the first name of an object std1
	std1.setfName("Jhon")
# Get input from user and set Last name to given input
	ln = input("Input Last Name:  ")
	std1.setLname(ln)

	std1.showDetails()

# calling the main function
main()
