class student():
    def __init__(self,lastname="Gurung", firstname="Amit", courseno=218989, course="computer science"):
        self._lastname = lastname
        self._firstname = firstname
        self._courseno = courseno
        self._course = course

std1 = student()	
print("lastname\t %s" %(std1._lastname))
print("firstname\t%s" %(std1._firstname))
print("courseno\t%d" %(std1._courseno))
print("course\t%s" %(std1._course))

std2 = student("Thapa","Gorkhe",34534,"Management")	
print("lastname\t %s" %(std2._lastname))
print("firstname\t%s" %(std2._firstname))
print("courseno\t%d" %(std2._courseno))
print("course\t%s" %(std2._course))

